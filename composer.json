{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "cuhuauth": {
            "type": "vcs",
            "url":  "git@github.com:squareborg/laravel-auth.git"
        }
    },
    "require": {
        "php": "^7.1.3",
        "cuhu/auth": "^0.0.2",
        "daverandom/resume": "^0.0.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "league/csv": "^9.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nesbot/carbon": "^2.25",
        "php-ffmpeg/php-ffmpeg": "^0.14.0",
        "spatie/data-transfer-object": "^1.8",
        "spatie/laravel-cors": "^1.5",
        "spatie/laravel-fractal": "^5.5",
        "spatie/laravel-permission": "^3.2.0",
        "spatie/laravel-sluggable": "^2.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
